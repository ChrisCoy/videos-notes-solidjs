{
  "manifest_version": 3,
  "name": "Youtube Notes",
  "description": "A simple tool to create notes for youtube videos.",
  "homepage_url": "https://github.com/ChrisCoy/Youtube-Video-Annotations",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extension",
    "default_icon": "icon.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "activeTab", "storage"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
