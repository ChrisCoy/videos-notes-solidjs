{
  "manifest_version": 3,
  "name": "Notes For Your Videos",
  "description": "A simple tool to create and share notes of videos or pages.",
  "homepage_url": "https://github.com/ChrisCoy/videos-notes-solidjs",
  "version": "1.0.2",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Notes for your videos",
    "default_icon": "icon.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "activeTab", "storage"],
  "icons": {
    "128": "icon.png"
  }
}
